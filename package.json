    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "element-ui": "^2.12.0",
    "express": "^4.17.1",
    "local-cors-proxy": "^1.1.0",
    "pdfmake": "^0.1.65",
    "ramda": "^0.26.1",
    "vue": "^2.6.10",
    "vue-i18n": "^8.15.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^5.2.3",
    "prettier": "^1.19.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
